<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:tnt="sap.tnt"
		controllerName="test_2.App" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Title">
		<content>
			<IconTabBar
				id="idIconTabBarMulti"
				expanded="{device>/isNoPhone}"
				class="sapUiResponsiveContentPadding">
				<items>
					<IconTabFilter
						icon="sap-icon://hint">
						<Text text="Info content goes here ..." />
					</IconTabFilter>
					<IconTabFilter
						icon="sap-icon://attachment"
						count="3">
						<Text text="Attachments go here ..." />
					</IconTabFilter>
					<IconTabFilter
						icon="sap-icon://notes"
						count="12">
						<Text text="Notes go here ..." />
					</IconTabFilter>
					<IconTabFilter
						icon="sap-icon://group">
						<Image src="https://media.cntraveler.com/photos/5aaac20c60ef3f51835c5be5/16:9/w_1440,c_limit/Rock-Of-Cashel-GettyImages-165559826.jpg" />
					</IconTabFilter>
				</items>
			</IconTabBar>			
		</content>
		<footer>
			<OverflowToolbar>
				<content>
					<ToolbarSpacer/>
					<OverflowToolbarButton type="Accept" text="Delete" icon="sap-icon://delete" />
					<ToolbarSeparator/>
					<Button icon="sap-icon://search" type="Transparent" />
					<Button icon="sap-icon://task" type="Transparent" />
					<Button icon="sap-icon://bell" type="Transparent" />
					<Button icon="test-resources/sap/tnt/images/avatar.png" type="Transparent" />
					<Button icon="sap-icon://grid" type="Transparent" />
					<OverflowToolbarButton type="Reject" text="Reject" icon="sap-icon://add" />
				</content>
			</OverflowToolbar>
		</footer>
	</Page>
</core:View>